{
  "name": "type-graph-node-react",
  "version": "1.0.0",
  "description": "A template for creating Node.js apps using Typescript as a language, GraphQL as a server and React as a client",
  "main": "build/server/index.js",
  "scripts": {
    "start": "node -r dotenv/config ./build/server/server.js",
    "lint": "eslint --ignore-pattern './build/*'",
    "codegen": "npx graphql-codegen --config .graphqlrc.yml",
    "start:prod": "NODE_ENV=production npm-run-all build:prod start",
    "start:dev": "NODE_ENV=development npm-run-all lint build:dev start",
    "build:prod": "webpack --config @webpack/webpack.prod.ts",
    "build:dev": "webpack --config @webpack/webpack.dev.ts",
    "watch:server": "nodemon --config nodemon/nodemon.server.json",
    "watch:graphql": "nodemon --config nodemon/nodemon.graphql.json",
    "watch": "npm run watch:server & npm run watch:graphql"
  },
  "keywords": [],
  "author": "soffyo",
  "license": "ISC",
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/react-router-dom": "^5.3.3",
    "compression": "^1.7.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "use-graphql-ts": "^1.0.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typed-document-node": "^2.2.8",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "class-validator": "^0.13.2",
    "connect-history-api-fallback": "^1.6.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "graphql-tag": "^2.12.6",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "type-fest": "^2.12.2",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.1",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  }
}
